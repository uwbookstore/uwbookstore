{
  "name": "bs5",
  "version": "1.0.0",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-optional-chaining"
    ]
  },
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css -w --no-source-map",
    "devserver": "live-server",
    "start": "npm-run-all --parallel watch:sass devserver",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.min.css --style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css",
    "build:js": "babel src -d build",
    "uglify:js": "uglifyjs-folder build -eo js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.0.0",
    "autoprefixer": "^10.4.15",
    "concat": "^1.0.3",
    "eslint": "^8.49.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-jquery": "^3.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.4.29",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.0.3",
    "sass": "^1.77.6",
    "uglify-js": "^3.17.4",
    "uglifyjs-folder": "^3.3.0"
  },
  "dependencies": {
    "holderjs": "^2.9.9",
    "live-server": "^1.2.2"
  }
}
